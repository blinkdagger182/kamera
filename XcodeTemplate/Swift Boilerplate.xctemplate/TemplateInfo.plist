<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Kind</key>
	<string>Xcode.Xcode3.ProjectTemplateUnitKind</string>
	<key>Identifier</key>
	<string>com.yourcompany.swiftboilerplate</string>
	<key>Ancestors</key>
	<array>
		<string>com.apple.dt.unit.applicationBase</string>
		<string>com.apple.dt.unit.iosBase</string>
	</array>
	<key>Concrete</key>
	<true/>
	<key>Description</key>
	<string>This template provides a starting point for a SwiftUI iOS application with authentication, networking, and other common features.</string>
	<key>SortOrder</key>
	<integer>1</integer>
	<key>NameOfInitialFileForEditor</key>
	<string>AppDelegate.swift</string>
	<key>Options</key>
	<array>
		<dict>
			<key>Identifier</key>
			<string>productName</string>
			<key>Required</key>
			<true/>
			<key>Name</key>
			<string>Product Name:</string>
			<key>Description</key>
			<string>Your app's name.</string>
			<key>Type</key>
			<string>text</string>
			<key>Default</key>
			<string>MyApp</string>
		</dict>
		<dict>
			<key>Identifier</key>
			<string>organizationName</string>
			<key>Name</key>
			<string>Organization Name:</string>
			<key>Description</key>
			<string>Your company's name.</string>
			<key>Type</key>
			<string>text</string>
			<key>Default</key>
			<string>___ORGANIZATIONNAME___</string>
		</dict>
		<dict>
			<key>Identifier</key>
			<string>bundleIdentifier</string>
			<key>Required</key>
			<true/>
			<key>Name</key>
			<string>Bundle Identifier:</string>
			<key>Description</key>
			<string>Your app's bundle identifier.</string>
			<key>Type</key>
			<string>text</string>
			<key>Default</key>
			<string>com.___ORGANIZATIONNAME___.___PACKAGENAME___</string>
		</dict>
		<dict>
			<key>Identifier</key>
			<string>usesCloudKit</string>
			<key>Name</key>
			<string>Include CloudKit support</string>
			<key>Description</key>
			<string>Adds CloudKit framework and capabilities to the project.</string>
			<key>Type</key>
			<string>checkbox</string>
			<key>Default</key>
			<string>false</string>
			<key>Units</key>
			<dict>
				<key>true</key>
				<array>
					<dict>
						<key>Targets</key>
						<array>
							<dict>
								<key>SharedSettings</key>
								<dict>
									<key>CLOUDKIT_ENABLED</key>
									<string>YES</string>
								</dict>
							</dict>
						</array>
					</dict>
				</array>
			</dict>
		</dict>
		<dict>
			<key>Identifier</key>
			<string>usesPushNotifications</string>
			<key>Name</key>
			<string>Include Push Notifications</string>
			<key>Description</key>
			<string>Adds Push Notification capabilities to the project.</string>
			<key>Type</key>
			<string>checkbox</string>
			<key>Default</key>
			<string>false</string>
			<key>Units</key>
			<dict>
				<key>true</key>
				<array>
					<dict>
						<key>Targets</key>
						<array>
							<dict>
								<key>SharedSettings</key>
								<dict>
									<key>PUSH_NOTIFICATIONS_ENABLED</key>
									<string>YES</string>
								</dict>
							</dict>
						</array>
					</dict>
				</array>
			</dict>
		</dict>
	</array>
</dict>
</plist> 